file = {:home/griessbaum/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bandrowski et al. - 2016 - The Resource Identification Initiative A cultural shift in publishing.pdf:pdf},
isbn = {2712005775},
