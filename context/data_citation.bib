file = {:home/griessbaum/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Starr et al. - 2015 - Achieving human and machine accessibility of cited data in scholarly publications.pdf:pdf},
isbn = {2376-5992 (Print)},
issn = {2376-5992},
journal = {PeerJ Computer Science},
keywords = {data citation},
