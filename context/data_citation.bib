title = {{Data Citation in Neuroimaging: Proposed Best Practices for Data Identification and Attribution}},
url = {https://www.frontiersin.org/article/10.3389/fninf.2016.00034 http://journal.frontiersin.org/Article/10.3389/fninf.2016.00034/abstract},
